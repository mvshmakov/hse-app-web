{
    "name": "hse-app-web",
    "version": "1.0.0",
    "description": "Web Interface for HSE APP",
    "keywords": [
        "hse",
        "hse-app",
        "schedule"
    ],
    "engines": {
        "node": "v9.11.x",
        "npm": "5.10.x"
    },
    "repository": {
        "type": "git",
        "url": "git://github.com/mvshmakov/hse-military-bot.git"
    },
    "bugs": {
        "url": "https://github.com/mvshmakov/hse-military-bot/issues"
    },
    "homepage": "https://github.com/vikpe/react-webpack-typescript-starter",
    "main": "express.js",
    "scripts": {
        "start": "npm run start-dev",
        "start-dev": "webpack-dev-server --config=configs/webpack/development.js",
        "debug": "export DEBUG='express:*' && npm run start-dev",
        "start-prod": "npm run build && node express.js",
        "build": "npm run lint && npm run clean-dist && webpack -p --config=configs/webpack/production.js",
        "clean": "rm -rfd node_modules && npm run clean-dist",
        "clean-dist": "rm -rfd dist",
        "lint": "tslint './src/**/*.ts*' --format stylish --force",
        "test": "jest --watch --coverage --config=configs/jest.json"
    },
    "dependencies": {
        "@vkontakte/icons": "^1.4.3",
        "@vkontakte/vkui": "^2.20.0",
        "@vkontakte/vkui-connect": "^1.1.3",
        "@vkontakte/vkui-connect-promise": "^0.1.19",
        "react": "^16.8.6",
        "react-dom": "^16.8.6",
        "throttle-typescript": "^1.0.1"
    },
    "devDependencies": {
        "@babel/cli": "^7.2.3",
        "@babel/core": "^7.4.0",
        "@babel/preset-env": "^7.4.1",
        "@babel/preset-react": "^7.0.0",
        "@types/jest": "^24.0.11",
        "@types/node": "^11.11.4",
        "@types/react": "^16.8.8",
        "@types/react-dom": "^16.8.2",
        "awesome-typescript-loader": "^5.2.1",
        "babel-loader": "^8.0.5",
        "css-loader": "^2.1.1",
        "express": "^4.16.4",
        "file-loader": "^3.0.1",
        "html-webpack-plugin": "^3.2.0",
        "image-webpack-loader": "^4.6.0",
        "jest": "^24.5.0",
        "node-sass": "^4.11.0",
        "react-hot-loader": "^4.8.0",
        "sass-loader": "^7.1.0",
        "style-loader": "^0.23.1",
        "tslint": "^5.14.0",
        "typescript": "^3.3.4000",
        "uglifyjs-webpack-plugin": "^2.1.2",
        "webpack": "^4.29.6",
        "webpack-cli": "^3.3.0",
        "webpack-config": "^7.5.0",
        "webpack-dev-middleware": "^3.6.1",
        "webpack-dev-server": "^3.2.1",
        "webpack-merge": "^4.2.1"
    }
}
